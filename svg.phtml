<script>
    $(function(){
        svg = {

            init: function() {
                svg.attachHandlers();
            },

            attachHandlers: function() {
                $('.dot').hover(svg.handleDotHover, svg.handleMouseLeave);
            },

            handleDotHover: function(event) {
                var dotNumber = svg.filterId($(event.target).attr('id'));
                var traceId = 'TRACE' + dotNumber;
                var pathLength = svg.getPathLength(traceId);
                console.log(pathLength);

                $('#' + traceId).addClass('animating');
                $('#' + traceId).css('stroke-dasharray', pathLength);
                $('#' + traceId).css('stroke-dashoffset', pathLength);
            },

            /**
             * Filters the number from the id
             * @var string id
             * @return int;
             */
            filterId: function(id) {
                return id.replace(/\D+/g, '');
            },

            /**
             * Get the total length of a path element
             * @var string traceId
             * @return int;
             */
            getPathLength: function(traceId) {
                var path = document.getElementById(traceId);
                return path.getTotalLength();
            },

            handleMouseLeave: function(event) {
                var dotNumber = svg.filterId($(event.target).attr('id'));
                var traceId = 'TRACE' + dotNumber;

                $('#' + traceId).removeClass('animating');
            }
        };

        svg.init();
    });

</script>
